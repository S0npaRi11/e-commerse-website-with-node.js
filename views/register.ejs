<%- include('partials/_navbar') %>
<main class="ps-main">


    <div class="ps-contact ps-section pb-80">
      <!--<div id="contact-map" data-address="New York, NY" data-title="Sky Store!" data-zoom="17"></div> -->
      <div class="ps-container" style="margin: 0 auto;">

<div class="row">
    <div class="col-lg-12">
        <h2 class="ps-section__title" data-mask="Register">- Register</h2>
    </div>

    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
    <div class="ps-section__header mb-50">
     
      <form class="ps-contact__form" action="/register" method="post">
        <div class="row">   
              <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 ">
                <div class="form-group">
                  <label>Name <sub>*</sub></label>
                  <input class="form-control" type="text" placeholder="" name="fname" id="fname">
                </div>
              </div>
              <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 ">
                <div class="form-group">
                  <label>Age <sub>*</sub></label>
                  <input class="form-control" type="text" placeholder="" name="age" id="age">
                </div>
              </div>
              <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 ">
                <div class="form-group">
                  <label>Email <sub>*</sub></label>
                  <input class="form-control" type="email" placeholder="" name="email" id="email">
                </div>
              </div>
              <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 ">
                <div class="form-group">
                  <label>Phone Number <sub>*</sub></label>
                  <input class="form-control" type="number" placeholder="" name="pno" id="pno">
                </div>
              </div>
              <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 ">
                <div class="form-group">
                  <label>Password <sub>*</sub></label>
                  <input class="form-control" type="password" placeholder="" name="password1" id="password1">
                </div>
              </div>
              <!-- <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 ">
                <div class="form-group">
                  <label>Re-enter Password <sub>*</sub></label>
                  <input class="form-control" type="password" placeholder="" name="password2" id="password2">
                </div>
              </div> -->
              <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 ">
                <div class="form-group">
                  <label>Postal Pin <sub>*</sub></label>
                  <input class="form-control" type="number" placeholder="" name="pin" id="pin">
                </div>
              </div>
              <div class="form-group mb-25">
                <label>Address <sub>*</sub></label>
                <textarea class="form-control" rows="6" name="address" id="address"></textarea>
              </div>
              <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
                <div class="form-group">
                  <button class="ps-btn">Register<i class="ps-icon-next"></i></button>
                </div>
              </div>
        </div>
      </form>
      <p> Already registered? <a href="/login"> Login here </a> </p>
    </div>
</div>
</div>
</div>
</main>

<%- include('partials/_footer') %>