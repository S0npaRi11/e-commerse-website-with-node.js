<%- include('./partials/_dashboard_nav') %>

<%- include('./partials/_mobile_nav') %>

<main class="ps-main">
    <div class="ps-container">
        <div class="ps-cart-listing ps-table--whishlist">
            <table class="table ps-cart__table">
                <thead>
                    <tr>
                    <th>All Products</th>
                    <th>Price</th>
                    <th>View</th>
                    <th> Purchase Date </th>
                    <th>Status</th>
                    </tr>
                </thead>
                <tbody>
                <% orders.forEach((item) => {  %>
                        <tr>
                            <td><a class="ps-product__preview" href="product-detail.html"><img class="mr-15" src="<%= item.image %>" alt=" <%= item.image %> "><%= item.productName %></a></td>
                            <td> &#8377; <%= item.productPrice %></td>
                            <td><a class="ps-product-link" href="/store/<%= item.productId %>">View Product</a></td>
                            <td> <%= item.orderDate %> </td>
                            <td> <%= item.orderStatus %> </td>
                        </tr>
                <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</main>


<%- include ('./partials/_footer') %>